h({ ...symbol, id: index }),\n    );\n    setCards(cardsArray);\n  }, []);\n\n  useEffect(() => {\n    if (openCards.length === 2) {\n      setDisabled(true);\n      checkCards();\n    }\n  }, [openCards]);\n\n  useEffect(() => {\n    if (\n      finishedCards.length !== 0 &&\n      finishedCards.length === cards.length / 2\n    ) {\n      onSetWinner(true);\n    }\n  });\n\n  const onCardClick = (symbol: Symbol) => {\n    if (symbol.isFlipped) {\n      return;\n    }\n    reverseCard(symbol);\n    setOpenCards((prevOpenCards) => [\n      ...prevOpenCards,\n      { ...symbol, isFlipped: true },\n    ]);\n  };\n\n  const reverseCard = (symbol: Symbol) => {\n    let flippedSymbol = { ...symbol, isFlipped: !symbol.isFlipped };\n    let cardsArray = [...cards];\n    cardsArray[symbol.id] = flippedSymbol;\n    setCards(cardsArray);\n  };\n\n  const reverseCards = () => {\n    let cardsArray = [...cards];\n    cardsArray[openCards[0].id] = { ...openCards[0], isFlipped: false };\n    cardsArray[openCards[1].id] = { ...openCards[1], isFlipped: false };\n    setCards(cardsArray);\n  };\n\n  const checkCards = () => {\n    if (openCards[0].name === openCards[1].name) {\n      setFinishedCards((prevFinishedCards: Symbol[]) => [\n        ...prevFinishedCards,\n        openCards[0],\n      ]);\n      setDisabled(false);\n    } else {\n      setTimeout(() => {\n        reverseCards();\n        setDisabled(false);\n      }, 700);\n    }\n    onScoreUpdate(score + 1);\n    setOpenCards([]);\n  };\n\n  return (\n    <BoardWrapper>\n      {cards.map((symbol: Symbol) => (\n        <GameCard\n          key={`${symbol.name}${Math.random()}`}\n          symbol={symbol}\n          clickHandler={() => onCardClick(symbol)}\n          disableCardClick={disableCardClick}\n        />\n      ))}\n    </BoardWrapper>\n  );\n};\n\nconst mapStateToProps = (state: RootState) => {\n  const score = state.userReducer.score;\n  return {\n    score,\n  };\n};\n\nconst mapDispatchToProps = (dispatch: Function) => {\n  return {\n    onScoreUpdate: (score: number) => dispatch(onScoreUpdate(score)),\n    onSetWinner: (winner: boolean) => dispatch(onSetWinner(winner)),\n  };\n};\n\nexport default connect(mapStateToProps, mapDispatchToProps)(GameBoard);\n","/Users/weronikafiedoruk/Documents/rzeczy/memory-app/src/redux/reducers/userReducer.ts",[],"/Users/weronikafiedoruk/Documents/rzeczy/memory-app/src/redux/actionTypes.ts",[],"/Users/weronikafiedoruk/Documents/rzeczy/memory-app/src/components/ui/BaseButton/base-button.styled.ts",[],"/Users/weronikafiedoruk/Documents/rzeczy/memory-app/src/components/Rating/rating.styled.ts",[],"/Users/weronikafiedoruk/Documents/rzeczy/memory-app/src/components/ui/ClickCounter/click-counter.styled.ts",[],"/Users/weronikafiedoruk/Documents/rzeczy/memory-app/src/components/ui/BaseInput/base-input.styled.ts",[],"/Users/weronikafiedoruk/Documents/rzeczy/memory-app/src/components/GameBoard/game-board.styled.ts",[],"/Users/weronikafiedoruk/Documents/rzeczy/memory-app/src/redux/actions/userActions.ts",[],"/Users/weronikafiedoruk/Documents/rzeczy/memory-app/src/redux/actions/ratingActions.ts",[],"/Users/weronikafiedoruk/Documents/rzeczy/memory-app/src/helpers/data.ts",[],"/Users/weronikafiedoruk/Documents/rzeczy/memory-app/src/helpers/helperFunctions.ts",[],"/Users/weronikafiedoruk/Documents/rzeczy/memory-app/src/components/ui/RatingBox/index.tsx",[],"/Users/weronikafiedoruk/Documents/rzeczy/memory-app/src/components/ui/GameCard/index.tsx",[],"/Users/weronikafiedoruk/Documents/rzeczy/memory-app/src/components/ui/RatingBox/rating-box.styled.ts",[],"/Users/weronikafiedoruk/Documents/rzeczy/memory-app/src/components/ui/GameCard/game-card.styled.ts",[],{"ruleId":"160","replacedBy":"161"},{"ruleId":"162","replacedBy":"163"},{"ruleId":"164","severity":1,"message":"165","line":26,"column":6,"nodeType":"166","endLine":26,"endColumn":8,"suggestions":"167"},{"ruleId":"164","severity":1,"message":"165","line":28,"column":6,"nodeType":"166","endLine":28,"end